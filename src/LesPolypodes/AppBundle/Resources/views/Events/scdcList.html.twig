List via SimpleCalDAV
<h2><span class="calLength">{{ result|length }}</span>
	Remote calendars{% if 0 < result|length  %} exist!{% endif %}

</h2>
	<table>
	{% for item in result %}
	    	<tr>
			<td><a id="link_{{ loop.index}}" href="{{ path('les_polypodes_app_list_event', {calendarName:item.calendar.displayname, serverName: app.request.get('serverName')}) }}">
	    		{{ item.calendar.displayname }} (ID: {{ item.calendar.calendarid }})
	    	</a>
				</td><td>count: {{ item.length }} events
				</td><td><a id="insert2_{{ loop.index }}" href="{{ path('les_polypodes_app_dev_insert', {calendarName:item.calendar.displayname, n:2, serverName: app.request.get('serverName')}) }}">
				Insert 2</a>
				</td><td><a id="insert10_{{ loop.index }}" href="{{ path('les_polypodes_app_dev_insert', {calendarName:item.calendar.displayname, n:10, serverName: app.request.get('serverName')}) }}">
				Insert 10</a>
				</td><td><a id="raw_{{ loop.index }}" href="{{ path('les_polypodes_app_list_event_raw', {calendarName:item.calendar.displayname, serverName: app.request.get('serverName')}) }}">
				Raw</a>
				</td><td><a id="delete_{{ loop.index }}" href="{# {{ path('les_polypodes_app_delete', {serverName: app.request.get('serverName')}) }} #}">
				Clean calendar</a>
	    </td></tr>
	{% endfor %}
</table>

